{
	"name": "vscode-rax-scripts",
	"displayName": "Rax Scripts",
	"description": "Development and build scripts for Rax project",
	"publisher": "Rax",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/raxjs/rax-vscode-extensions.git"
	},
	"icon": "assets/logo.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/index.js",
	"contributes": {
		"commands": [
			{
				"command": "rax.start",
				"title": "Rax Scripts: start a development server",
				"category": "Rax"
			},
			{
				"command": "rax.debug",
				"title": "Rax Scripts: run and debug",
				"category": "Rax"
			},
			{
				"command": "rax.build",
				"title": "Rax Scripts: Build your project",
				"category": "Rax"
			}
		]
	},
	"extensionDependencies": [
		"weex.weex-debugger",
		"msjsdiag.debugger-for-chrome",
		"auchenberg.vscode-browser-preview"
	],
	"scripts": {
		"build": "cd client && build-scripts build",
		"start": "cd client && build-scripts start"
	},
	"dependencies": {
		"ejs": "^3.0.1",
		"rax": "^1.1.0",
		"rax-app": "^2.0.0",
		"rax-document": "^0.1.0"
	},
	"devDependencies": {
		"build-plugin-rax-multi-pages": "^0.1.2",
		"build-plugin-rax-app": "^2.0.0",
		"@alib/build-scripts": "^0.1.0"
	}
}