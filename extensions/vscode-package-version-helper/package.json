{
  "name": "vscode-package-version-helper",
  "displayName": "Package Version Helper",
  "description": "Show npm package's version information in you code.",
  "publisher": "Rax",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raxjs/rax-vscode-extensions.git"
  },
  "icon": "assets/logo.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "package.version.check",
        "title": "Check Package Version"
      },
      {
        "command": "package.open",
        "title": "Open package.json"
      }
    ],
    "configuration": {
			"type": "object",
			"title": "Package Version Helper",
			"properties": {
				"packageVersion.registry": {
					"type": "string",
					"default": "https://registry.npmjs.org/",
					"scope": "window",
					"description": "Set npm registry."
				}
			}
		}
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "line-column": "^1.0.2",
    "semver": "^7.3.2"
  }
}
