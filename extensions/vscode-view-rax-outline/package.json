{
	"name": "vscode-view-rax-outline",
	"displayName": "View Rax Outline",
	"description": "Add a view in Rax Activity Bar that shows the Abstract Syntax Tree.",
	"publisher": "Rax",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/raxjs/rax-vscode-extensions.git"
	},
	"icon": "assets/logo.png",
	"activationEvents": [
		"onView:raxOutline"
	],
	"main": "./src/index.js",
	"contributes": {
		"commands": [
			{
				"command": "rax.start",
				"title": "Start a development server",
				"category": "Rax"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "rax",
					"title": "Rax",
					"icon": "assets/viewIcon.png"
				}
			]
		},
		"views": {
			"rax": [
				{
					"id": "raxOutline",
					"name": "Outline"
				}
			]
		}
	}
}